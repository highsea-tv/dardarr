{
    "title": "Internationalization",
    "id": "https://highsea-tv.github.io/dardarr/i18n.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Dardarr localization schema",
    "type": "object",
    "properties": {
        "language": {
            "type": "string"
        },
        "install_page": {
            "type": "object",
            "properties": {
                "title": {
                    "type": "string"
                },
                "button": {
                    "type": "string"
                }
            },
            "required": ["title", "button"]
        }
    },
    "required": ["install_page"]
}
